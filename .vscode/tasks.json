{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "rust: cargo run",
      "type": "cargo",
      "command": "run",
      "args": ["--", "-v", "load", "sample/sample.arw"],
      "problemMatcher": [
        "$rustc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "rust: cargo test",
      "type": "cargo",
      "command": "test",
      "problemMatcher": [
        "$rustc"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      }
    }
  ]
}